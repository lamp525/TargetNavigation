@using System.Configuration;
@{
    var pageNum = ConfigurationManager.AppSettings["pageNum"].ToString();
    var userPage = ConfigurationManager.AppSettings["userPage"].ToString();

        //设置量质时的最大值
    var completeQuantity = ConfigurationManager.AppSettings["maxQuantity"].ToString();

    var completeQuality = ConfigurationManager.AppSettings["maxQuality"].ToString();
    var completeTime = ConfigurationManager.AppSettings["maxTime"].ToString();

        //设置错误信息文件
    var MessagePath = ConfigurationManager.AppSettings["MessagePath"].ToString();

        //密码错误次数验证码
    var InputErrorValidate = ConfigurationManager.AppSettings["InputErrorValidate"].ToString();

        //上传文件路径
    var NewsUpLoadPath = ConfigurationManager.AppSettings["NewsUpLoadPath"].ToString();

    var DocumentUpLoadPath = ConfigurationManager.AppSettings["DocumentUpLoadPath"].ToString();
    var PlanUpLoadPath = ConfigurationManager.AppSettings["PlanUpLoadPath"].ToString();
    var MineUpLoadPath = ConfigurationManager.AppSettings["MineUpLoadPath"].ToString();
    var HeadImageUpLoadPath = ConfigurationManager.AppSettings["HeadImageUpLoadPath"].ToString();
    var IMUploadPath = ConfigurationManager.AppSettings["IMUploadPath"].ToString();
    var ObjectiveUploadPath = ConfigurationManager.AppSettings["ObjectiveUploadPath"].ToString();
    var FlowIndexUploadPath = ConfigurationManager.AppSettings["FlowIndexUploadPath"].ToString();
    var MeetingUpLoadPath = ConfigurationManager.AppSettings["MeetingUpLoadPath"].ToString();
    var ConvertFilePath = ConfigurationManager.AppSettings["ConvertFilePath"].ToString();

        //IM通讯服务
    var IMHost = ConfigurationManager.AppSettings["IMHost"].ToString();

        //模板路径
    var PlanTemplate = ConfigurationManager.AppSettings["PlanTemplate"].ToString();
    }

<!-- 本页所需的样式 开始 -->
<link rel="stylesheet" href="../../Styles/SystemSetting/systemSetting.css"/>
<!-- 本页所需的样式 结束 -->

<!-- 页面内容 开始 -->

<div class="row">
    <div id="dateContainer" class="col-xs-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <div class="panel-title">
                    后台管理
                </div>
            </div>
            <div class="panel-body pos-r">
                <table class="table table-hover" id="systemSetting">
                    <thead>
                    <tr>
                        <th>系统项目</th>
                        <th>配置内容</th>
                        <th>操作</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr class="set-item" id="DocumentUpLoadPath">
                        <td style="width:35%;">文档上传路径</td>
                        <td style="width:50%;text-overflow:initial;">
                            <span class="set-display text-overflow" title="@DocumentUpLoadPath">@DocumentUpLoadPath</span>
                            <input type="text" class="set-text" id="DocumentUpLoadPath2" value="@DocumentUpLoadPath"/>
                        </td>
                        <td style="width:15%;"><a href="#" class="set-change">修改</a></td>
                    </tr>
                    <tr class="set-item" id="PlanUpLoadPath">
                        <td>计划附件路径</td>
                        <td>
                            <span class="set-display text-overflow" title="@PlanUpLoadPath">@PlanUpLoadPath</span>
                            <input type="text" class="set-text" id="PlanUpLoadPath2" value="@PlanUpLoadPath"/>
                        </td>
                        <td><a href="#" class="set-change">修改</a></td>
                    </tr>
                    <tr class="set-item" id="MeetingUpLoadPath">
                        <td>会议资料路径</td>
                        <td>
                            <span class="set-display text-overflow" title="@MeetingUpLoadPath">@MeetingUpLoadPath</span>
                            <input type="text" class="set-text" id="MeetingUpLoadPath2" value="@MeetingUpLoadPath"/>
                        </td>
                        <td><a href="#" class="set-change">修改</a> </td>
                    </tr>
                    <tr class="set-item" id="FlowIndexUploadPath">
                        <td>流程附件路径</td>
                        <td>
                            <span class="set-display text-overflow" title="@FlowIndexUploadPath">@FlowIndexUploadPath</span>
                            <input type="text" class="set-text" id="FlowIndexUploadPath2" value="@FlowIndexUploadPath"/>
                        </td>
                        <td><a href="#" class="set-change">修改</a></td>
                    </tr>
                    <tr class="set-item" id="InputErrorValidate">
                        <td>登录验证密码输错次数</td>
                        <td>
                              <span class="set-display text-overflow" title="@InputErrorValidate">@InputErrorValidate</span>
                            <input type="text" class="set-text" id="InputErrorValidate2" value="@InputErrorValidate"/>
