                        <td><a href="#" class="set-change">修改</a></td>
                    </tr>
                    <tr class="set-item" id="completeQuantity">
                        <td>设置完成数量</td>
                        <td>
                              <span class="set-display text-overflow" title="@completeQuantity">@completeQuantity</span>
                            <input type="text" class="set-text" id="completeQuantity2" value="@completeQuantity"/>
