<script type="text/template" id="actionTarget">
    <%for(var i = 0, len = list.length; i < len; i++ ){ %>
    <% var item = list[i] %>
    <% var per = { 1: 90, 2: 90, 3: item.progress ? item.progress : 90, 4: 90, 5: item.progress ? item.progress : 90 }[item.status] %>
    <% var colorFul = { 1: '#be1d9a', 2: '#fbab11', 3: '#64d8ae', 4: '#57acdb', 5: '#58b557' }[item.status] %>
    <% var text = {1:'待提交',2:'待审核',3:item.progress?item.progress+'%':'0%',4:'待确认',5:item.progress?item.progress+'%':'0%'}[item.status] %>
    <div class="wrapAll parentObject">
    <div class="targetListAll">
        <div class="targetHead">
            互联网产业 - 目标导航事业部
        </div>
        <div class="targetBody">
           <div class="leftTarget">
               <div class="circle circleTarget" style="left:0">
                  <input data-text = <%=text %> data-font-weight="normal" data-max=90 data-angleArc = 324 data-fgColor=<%=(!item.progress && (item.status == 3 || item.status == 5)) ? '#e3e3e3' : colorFul %> class="knob" data-readonly="true"  data-skin="tron" data-displayinput="true" value=<%=per %> >

               </div>
           </div>
            <div class="rightTarget">
                <div class="col-xs-5 targetInfo">
                    <ul>
                        <li>目标名称：<%=item.objectiveName %></li>
                        <li>起止时间：<%=item.startTime.replace('T','至').substring(0,11) %>-<%=item.endTime.substring(0,10) %></li>
                        <li>奖励基数：<%=item.bonus %></li>
                        <li>确认人：<%=item.confirmUserName %></li>
                    </ul>
                </div>
                <div class="col-xs-5 targetInfo">
                    <ul>
                        <li>理想值：<%=item.expectedValue %></li>
                        <li>指标值：<%=item.objectiveValue %></li>
                        <li>警戒时间：<%=item.alarmTime?item.alarmTime.substring(0,10):'' %></li>
                        <li>考核类型：<%=item.objectiveType == 1?'金融':'时间' %></li>
                    </ul>
                </div>
                <div class="col-xs-2 power targetInfo">
                    <%=item.weight?'权重：'+item.weight+'%':'' %>
                </div>
                <div class="lastShowTarget">
                    <ul>
                        <li class="liPower planHide" term=<%=item.objectiveId %> >授权</li>
                        <li class="liConfirm planHide" term=<%=item.objectiveId %> >提交确认</li>
                        <li class="liDel planHide" term=<%=item.objectiveId %> flag=1>删除</li>
                        <li class="liRevocation planHide" term=<%=item.objectiveId %>  flag=1>撤销</li>
                        <li class="liModify planHide" term=<%=item.objectiveId %> >修改</li>
                        <li class="liCommit planHide" term=<%=item.objectiveId %> >提交</li>
                        <li class="liCheck planHide" term=<%=item.objectiveId %> >审核</li>
                        <li class="liSure planHide" term=<%=item.objectiveId %> >确认</li>
                        <li class="liOpen" term=<%=item.objectiveId %> >展开</li>
                        <li class="liDetail" term=<%=item.objectiveId %> >详情</li>
                        <li class="liPowerCancel planHide" term=<%=item.objectiveId %>  flag=1>取消授权</li>
                        
                    </ul>
                </div>
            </div>
        </div>
    </div>
    </div>
    <%} %>

</script>


