                                     <button class="input-group-addon commonBtn" style="height: 33px;">
                                       <span class="responseCorn" ></span>
                                    </button>
                                </div>
                                <label class="control-label">确认人：</label>
                                <div class="input-group">
                                    <input type="button" class="form-control" id="confirmPerson">
                                         <span class="input-group-addon commonBtn">
                                           <span class="confirmCorn" ></span>
                                        </span>
                                </div>
                            </div>

                            <div class="row"><h4 class="col-xs-6"> 相关文档 </h4>
                                <span class="modifyDocumentAdd  pull-right"><input type="file"  class="fileUploadBtn" id="fileUpload" name="files[]" multiple ></span>
                                <div class="progress"><div class="bar" style="width:0%;"></div></div>
                            </div>
                            <hr class="noMarginTop"  />
                            <table class="table table-hover" id="modifyObjectDocument">
                            </table>

                            <h4>意见</h4>
                            <hr class="noMarginTop"/>
                            <textarea class="form-control" id="modifyMessage"  rows="2" maxlength="250"></textarea>

                            <h4>操作日志</h4>
                            <hr class="noMarginTop"/>
                            <table class="table table-hover" id="modifyObjectLog">
                            </table>
                        </div>
                        
                     <div class="modal-footer fifty-fifty">
                            <a class="btn btn-transparency " data-dismiss="modal" id="objectModifyCancel">取消</a>
                            <a class="btn btn-transparency " term="1" id="objectModifySave">保存</a>
                            <a class="btn btn-transparency " term="2" id="objectModifyCommit">提交</a>
