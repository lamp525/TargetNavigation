<script type="text/template" id="planTarget">
    <%for(var i = 0, len = list.length; i < len; i++ ){ %>
    <% var item = list[i] %>
    <div class="col-xs-6 planModule planListAll">
        <div class="planWrap">
            <div class="wrapInfo">
                <div class="circlePer planPer">
                   <%if((item.status == 20 || item.status == 40) && item.stop == 0) { %>
                     <input term=<%=item.planId %> data-readonly="false" data-max=90 data-angleArc = 324 data-fgColor="#be1d9a" class="knob"  data-skin="tron" data-displayinput="true" value=<%=item.progress?item.progress:0 %> >
                   <%} else if((item.status == 0 || item.status == 15) && item.stop == 0) { %>
                     <input data-text = "待提交" data-font-weight="normal" data-max=90 data-angleArc = 324 data-fgColor="#57acdb" class="knob" data-readonly="true"  data-skin="tron" data-displayinput="true" value=<%=item.progress?item.progress:0 %> >
                   <%} else if(((item.status == 10 || item.status == 25) && item.stop == 0) || item.stop == 10) { %>  
                       <input  data-max=90 data-angleArc = 324 data-text = "待审核" data-font-weight="normal" data-fgColor="#e00e16" class="knob" data-readonly="true"  data-skin="tron" data-displayinput="true" value=<%=item.progress?item.progress:0 %> >   
                   <%} else if(item.status == 30 && item.stop == 0) { %>
                       <input  data-max=90 data-angleArc = 324 data-text = "待确认" data-font-weight="normal" data-fgColor="#fbab11" class="knob" data-readonly="true"  data-skin="tron" data-displayinput="true" value=<%=item.progress?item.progress:0 %> >         
                   <%} else if(item.status == 90 && item.stop == 0) { %>
                         <input data-fgColor="#58b557" class="knob" data-readonly="true"  data-skin="tron" data-displayinput="true" value=<%=item.progress?item.progress:0 %> > 
                   <%} else if(item.stop == 90){ %>
                        <input data-fgColor="#49dca7" class="knob" data-readonly="true"  data-skin="tron" data-displayinput="true" value=<%=item.progress?item.progress:0 %>>
                   <% } %>

                </div>
                <div class="detailInformation">
                    <ul>
                        <li>执行方：<%=item.executionMode %></li>
                        <li>输出结果：<%=item.eventOutput %></li>
                        <li>计划完成时间：<%=item.endTime.substring(0,10) %></li>
                        <li>确认人：<%=item.confirmUserName %></li>
                        <li class="impotrtant">重要度：<%for (var j = 1;j<=5;j++){ %> <% var star= j<=item.importance?"fa-star":"fa-star-o"  %><i class="fa <%=star %>"></i> <%} %></li>
                        <li class="exigence">紧急度：<%for (var j = 1;j<=5;j++){ %> <% var star= j<=item.urgency?"fa-star":"fa-star-o"  %><i class="fa <%=star %>"></i> <%} %></li>
                        <li class="hard">难易度：<%for (var j = 1;j<=5;j++){ %> <% var star= j<=item.difficulty?"fa-star":"fa-star-o"  %><i class="fa <%=star %>"></i> <%} %></li>
                    </ul>
                </div>
            </div>
            <div class="wrapDate">
                <span> <%= item.create %></span> <span><%= item.createTime %></span>
            </div>
        </div>
        <div class="bottomHover">
            <ul>
                <li class="decPlan planHide">分解</li>
                <li class="transimitPlan planHide">转办</li>
                <li class="modification planHide">修改</li>
                <li class="suspend planHide">终止</li>
                <li class="submit planHide">提交</li>
                <li class="submitTo planHide">提交</li>
                <li class="audit planHide">审核</li>
                <li class="revocation planHide">撤销</li>
                <li class="confirmPlan planHide">确认</li>
                <li class="restart planHide">重新开始</li>
                <li class="planDetail" withfront="0" collplan="0"  status="0" stop="0" isloop="0" initial="1">详情</li>
            </ul>
        </div>
    </div>
    <%} %>

</script>
