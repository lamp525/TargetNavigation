@{
    var height = Convert.ToInt32(ViewBag.Height);   
}
<script src="~/Scripts/plan/SubmitConfirm.js"></script> 
<div class="uploadBox" style="position: absolute; width: 360px; top: 0px; left: 560px;height:100%; padding: 0 20px;">
    <div style="height: 30px; line-height: 30px; border-bottom: solid 1px #e3e3e3; font-size: 16px">
        完成情况
    </div>
    <div class="uploadArea" style="margin: 10px 0; height: auto; border-bottom: 1px solid #bbb;">
        <div class="allProcessBar">
            <span>完成情况</span>
            <div class="processBar">
                <div id="slider"></div>
            </div>
            <span class="degree">90%</span>
        </div>
        <div class="file_upload">
            <div class="fileinput_frame">
                <span class="uploadTag">上传附件：</span>
                <div class="fileinput-button">
                    <div class="fileinput-button_l"></div>
                    <span>上传</span>
                    <input id="fileupload" type="file" name="files[]" multiple>
                    <div class="fileinput-button_r"></div>
                </div>
                <div class="files">
                    <ul>
                           @*<li>
                                <div class="up_progress"></div>
                                <span class="file">设计说明
                                    <span>x</span>
                                </span>
                            </li>
                            <li>
                                <div class="up_progress"></div>
                                <span class="file">设计说明
                                    <span>x</span>
                                </span>
                            </li>
                            <li>
                                <div class="up_progress"></div>
                                <span class="file">设计说明
                                    <span>x</span>
                                </span>
                            </li>*@
                        </ul>
                </div>
            </div>
        </div>

    </div>
    <div class="finishTime">
        <span>完成时间：</span>
        <input type="text" id="time" value=""  onkeyup="if(this.value.length==1){this.value=this.value.replace(/[^1-9]/g,'')}else{this.value=this.value.replace(/\D/g,'')}"  onafterpaste="if(this.value.length==1){this.value=this.value.replace(/[^1-9]/g,'0')}else{this.value=this.value.replace(/\D/g,'')}"/>
        分钟 x
                    <input type="text" id="num" value="1"  onkeyup="if(this.value.length==1){this.value=this.value.replace(/[^1-9]/g,'')}else{this.value=this.value.replace(/\D/g,'')}"  
                                    onafterpaste="if(this.value.length==1){this.value=this.value.replace(/[^1-9]/g,'0')}else{this.value=this.value.replace(/\D/g,'')}"/>件 = <span class="allTime" style="color: #58b557"  >0</span> 分钟
    </div>
    <div class="uploadOperate">
        <span style="margin-left: 10px;" id="submitUploadFile">提交</span>
        <span id="cancelUploadFile">取消</span>
    </div>
</div>
<input id="submitconfirm" type="hidden" value="@height" />

