<!-- 本页调用的JS 开始 -->
<script>
    $(function () {
        var hrefReg = /^#|^javascript/i;
        $(document).delegate("a", "click", function (e) {
            if ($(this).hasClass('toNew')) {
                return;
            }
            e.preventDefault();
            var href = $(this).attr("href");
            if (href && !hrefReg.test(href)) {
                loadViewToMain(href);
            }
        });
        var $nav = $("#mainNav");
        $nav.find("a").click(function () {
            if ($(this).hasClass('toNew')) {
                return;
            }
            if ($(this).parent().is(".active")) {
                return false;
            }
            $nav.find(".active").removeClass("active");
            $(this).parent().addClass("active");
        })
        var lastUrl = sessionStorage.getItem("currentUrl");
        if (lastUrl) {
            loadViewToMain(lastUrl);
        } else {
            $nav.find("[def]>a").click();
        }
    });
</script>
<!-- 本页调用的JS 结束 -->
