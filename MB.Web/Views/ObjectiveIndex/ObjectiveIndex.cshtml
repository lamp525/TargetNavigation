                 <h4 class="modal-title" id="objectRoleLabel">目标规则</h4>
            </div>
            <div class="tab-content" >
                <div role="tabpanel" id="detailViewTab" class="tab-pane active">
                    <div class="modal-body" style="overflow-y: auto;overflow-x: hidden;height:590px">

                    </div>
                </div>
                <div role="tabpanel" id="FormularViewTab" class="tab-pane ">
                    <div class="modal-body" style="overflow-y: auto;overflow-x: hidden;height:590px">

                    </div>
                </div>
            </div>
            <!--右边弹窗画面-->
            <div class="rightModal">
                <h4 id="rightTitle" style="margin-bottom: 7px;" ></h4>
                <hr/>
                <div id="rightContent">

                </div>
            </div>

        </div>

    </div>
</div>
