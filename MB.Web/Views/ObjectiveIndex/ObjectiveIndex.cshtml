<!-- 页面内容 开始 -->
<div class="row">
        <div id="objectContainer" class="col-xs-9">
            <div class="panel panel-default" id="filterBox">
                <div class="panel-heading" id="filter_selected">
                    <div class="row">
                       <span  class="col-xs-1">已选条件</span>
                       <ul class="list-inline selectedCondition col-xs-10" >

                       </ul>
                        <a href="#" class="pull-right" id="cleanCondition">清空</a>
                    </div>
                </div>
                <div  class="panel-body" id="specific">
                   <div class="row">
                       <span class="col-xs-1">目标对象：</span>
                       <div class="col-xs-2">
                          <div class="dropdown" id="object_target">
                              <span class="dropdown-toggle" data-toggle="dropdown" data-delay="50" aria-expanded="false">
                                  <span id="orgPersonSelect" term="1" >全部</span>
                                  <span class="upPic"></span>
                              </span>
                              <ul class="dropdown-menu">
                                  <li><a href="#" term="0">全部</a></li>
                                  <li><a href="#" term="1"  data-toggle="modal" data-target="#department_modal" >架构目标</a></li>
                                  <li><a href="#" term="2" >人员</a></li>
                              </ul>
                          </div>
                       </div>
                   </div>
                    <div class="row" id="statusCondition">
                        <span class="col-xs-1">状态：</span>
                        <ul class="list-inline col-xs-6">
                            <li><a  term="1" href="#">待提交</a></li>
                            <li><a  term="2" href="#">待审核</a></li>
                            <li><a  term="3"  href="#">审核通过</a></li>
                            <li><a  term="4" href="#">待确认</a></li>
                            <li><a  term="5" href="#">已完成</a></li>
                        </ul>
                    </div>
                   <div class="row" id="timeSelect">
                       <span class="col-xs-1">时间 : </span>
                       <ul class="list-inline">
                           <li><a href="#" term="1" >近一月</a></li>
                           <li><a href="#" term="2" >近一周</a></li>
                           <li>
                               <div class="form-group form-inline">
                                   <label  for="startTime">开始日期 </label>
                                   <div class="input-group">
                                       <input type="button" class="form-control"  id="startTime" placeholder="开始日期">
                                       <span class="input-group-addon">
                                           <span class="dateCorn" ></span>
                                       </span>
                                   </div>
                                   <span style="font-size:16px;">-</span>
                                   <label for="endTime" >结束日期 </label>
                                   <div class="input-group" >
                                       <input type="button" class="form-control" id="endTime" placeholder="结束日期">
                                       <span class="input-group-addon">
                                           <span class="dateCorn" ></span>
                                       </span>
                                   </div>
                               </div>
                           </li>
                           <li><a href="#" term="3" id="timeSure">确定</a></li>
                       </ul>
                   </div>
                   <div class="drawer-handle chooseCondition" id="filterbox_shrink">
                        看完了，点我收起来吧
                   </div>
                </div>
            </div>
            <div class="panel panel-default" style="margin-bottom: 0">
                <div class="panel-heading object">
                    <div class="panel-title">
                        <ul class="nav nav-tabs" >
                            <li role="presentation" class="active listObj"><a href="#myObject" aria-controls="myObject" role="tab" id="myObjectTab" >我的目标</a></li>
                            <li role="presentation" class="listObj"><a href="#subObject"  aria-controls="subObject"  id="subObjectTab" role="tab" >下属目标</a></li>
                            <li class="nav-right" >
                                <ul class="list-inline" >
                                    <li><a href="#" class="chooseCondition listObj">筛选</a></li>
                                    <li><a  class="listShow TabActiveColor" href="#"  aria-controls="subObject"   role="tab" data-toggle="tab" id="punckListTab">列表</a></li>
                                    <li  style="margin-left: -15px;"><span>/</span></li>
                                    <li  style="margin-left: -15px;"><a href="#gantList" class="listShow"  aria-controls="subObject" role="tab" data-toggle="tab" id="gantListTab">甘特图</a></li>
                                </ul>
                            </li>
                        </ul>
                        <div id ="objectKind" class="listObj">
                            <ul class="list-inline">
                                <!--&lt;!&ndash;//快捷查询，忽略其他筛选条件：1：待提交 2：待审核 3：审核通过 4：待确认 5：已完成 6、超时 int&ndash;&gt;-->
                                <!--<li term="1"><a  href="#">待提交 : </a><span style="color:#be1d9a"></span></li>-->
                                <!--<li term="2"><a  href="#">待审核 : </a><span style="color:#fbab11"></span></li>-->
                                <!--<li term="3"><a  href="#">审核通过 : </a><span style="color:#64d8ae"></span></li>-->
                                <!--<li term="4"><a  href="#">待确认 : </a><span style="color:#57acdb"></span></li>-->
                                <!--<li term="5"><a  href="#">已完成 : </a><span style="color:#58b557"></span></li>-->
                                <!--<li term="6"><a  href="#">已超时 : </a><span style="color:#e02215"></span></li>-->
                            </ul>
                        </div>
                    </div>
                </div>
                <div  class="panel-body">
                    <div class="tab-content" id="listTabContent">
                        <!--我的目标-->
                        <div class="tab-pane active" role="tabpanel" id="myObject">

                        </div>
                        <!--下属目标-->
                        <div class="tab-pane" role="tabpanel" id="subObject">

                        </div>

                        <!--甘特图-->
                        <div class=""  id="gantList" style="width:100%">
                            <div id="ganttOpt">
                                <a href="javascript:;"><i style="cursor: pointer;margin-left:10px;" class="glyphicon glyphicon-chevron-left viewDown"></i></a><a
                                    href="javascript:;"><i style="cursor:pointer;margin-left: 10px;"  class="viewUp glyphicon glyphicon-chevron-right"></i></a><a  href="javascript:;" class="pull-right active setMonth">月</a><span class="pull-right active">/</span><a href="javascript:;" class="pull-right setYear">年</a>
                            </div>
                             <div id="workSpace" style="padding:0px;overflow-y:hidden; overflow-x:hidden;border:1px solid #e5e5e5;position:relative;margin:0 5px;height:100%;width:100%">
                                <div class="leftYear viewDown"><i class="glyphicon glyphicon-chevron-left"></i></div><div class="rightYear viewUp"><i class=" glyphicon glyphicon-chevron-right"></i></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div id="sidebarContainer" class="col-xs-3">
            <div class="sidebar">
                <a type="button" class="btn btn-block btn-warning" id="newObjectiveBtn">新建目标</a>
                 @Html.Partial("~/Views/Shared/_UserInfo.cshtml")
                <div class="panel panel-default panel-chart" id="object_statistics">
                    <div class="panel-heading">目标进程</div>
                    <div class="panel-body pos-r chart-container" style="padding-bottom:0px">
                        <div class="chart-content" id="object_chart">

                        </div>
                        <div class="chart-center"></div>
                        <div class="month text-center" style="margin-top: 13px;">
                           <span class="arrowsBBLCom pull-left" style="margin-top: -1px;"></span>
                           <span class="text"></span>
                           <span class="arrowsBBRCom pull-right" style="display: none"></span>
                        </div>
                    </div>
                    <div class="panel-footer">
                        <ul class="list-inline chart-label">
                        </ul>
                    </div>
                </div>
             </div>
        </div>
    </div>

<!-- 页面内容 结束 -->

