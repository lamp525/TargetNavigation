<div class="modal fade" tabindex="-1"  role="dialog" aria-labelledby="object_modify_modal" id="object_modify_modal" aria-hidden="true" data-backdrop="false" data-keyboard="false">
    <div class="modal-dialog" >
        <div class="modal-content">
            <div class="modal-header" style="height:35px">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span></button>
                <a href="#modifyView" aria-controls="modifyView" role="tab" data-toggle="tab"><h4 class="modal-title" id="objectModifyTab">目标修改</h4></a>
                <a href="#modifyFormularView" aria-controls="modifyFormularView" role="tab" data-toggle="tab"><h4 class="modal-title disabledColor" id="objectModifyRole">目标规则</h4></a>
            </div>

             <div class="tab-content" >
                <div role="tabpanel" id="modifyView" class="tab-pane active">              
                     <div class="modal-body" style="overflow-y: auto;overflow-x: hidden;height:560px">
                            <h4 style="margin-top: 0px;">目标内容</h4>
                            <hr/>
                            <form class="borderTop" id="modifyObjectContent">
                                <div class="form-inline">
                                    <label for="objectName" class="control-label" style="">目标名称:</label>
                                    <input type="text" id="objectName" class="form-control" maxlength="50">
                                    <label for="modifyObjectInput" class="control-label  marginSm">目标对象:</label>
                                    <div class="dropdown" id="modifyObjectInput">
                                        <button class="btn btn-default dropdown-toggle dropdownBtn" data-toggle="dropdown" data-delay="50" role="button" aria-expanded="false">
                                            <span chooseId="" class="textOverFlow" style="max-width:155px"></span>
                                            <span class="pull-right upPic"></span>
                                        </button>
                                        <ul class="dropdown-menu" role="menu">
                                            <li><a herf='#' term='1'>架构</a></li>
                                            <li><a herf='#' term='2'>人员</a></li>
                                        </ul>
                                    </div>
                                    <!--<input type="text" class="form-control" id="modifyObjectInput" maxlength="50">-->
                                </div>
                                <div class="form-inline">
                                    <label for="objectBonus" class="control-label">奖励基数:</label>
                                    <input type="text" id="objectBonus" class="form-control"   maxlength="18">
                                    <label for="modifyWeight" class="control-label  marginSm">权重</label>
                                    <input type="text" class="form-control" id="modifyWeight"  maxlength="18" >
                                </div>
                                <div class="form-inline">
                                    <label for="modifyCheckType" class="control-label">考核类型:</label>
                                    <div class="dropdown" id="modifyCheckType">
                                        <button class="btn btn-default dropdown-toggle dropdownBtn" data-toggle="dropdown" data-delay="50" role="button" aria-expanded="false">
                                            <span></span>
                                            <span class="pull-right upPic"></span>
                                        </button>
                                        <ul class="dropdown-menu" role="menu">
                                            <li><a herf='#' term='1'>金额</a></li>
                                            <li><a herf='#' term='2'>时间</a></li>
                                            <li><a herf='#'  term='3'>数字</a></li>
                                        </ul>
                                    </div>
                                    <label class=" control-label  marginSm">指标值:</label>
                                    <div id="targetInput"  class="modifyObjectValue">
                                        <div class="input-group">
                                            <input type="text" class="form-control" id="targetMoney" maxlength="20">
                                            <span class="input-group-addon">
                                                <span class="glyphicon glyphicon-yen" ></span>
                                            </span>
                                        </div>
                                        <div class="input-group">
                                            <input type="button" class="form-control" id="targetTime" placeholder="日期">
                                            <span class="input-group-addon">
                                               <span class="dateCorn" ></span>
                                            </span>
                                        </div>
                                        <input type="text" class="form-control" id="targetNumber" maxlength="20" oninput="value=value.replace(/[^\d]/g,'')">
                                    </div>
                                </div>
                                <div class="form-inline">
                                    <label  class="control-label">理想值:</label>
                                    <div  id="expectInput"  class="modifyObjectValue">
                                        <div class="input-group">
                                            <input type="text" class="form-control" id="expectMoney" maxlength="20" >
                                            <span class="input-group-addon" >
                                               <span class="glyphicon glyphicon-yen"></span>
                                            </span>
                                        </div>
                                        <div class="input-group">
                                            <input type="button" class="form-control" id="expectTime" placeholder="日期">
                                            <span class="input-group-addon">
                                                <span class="dateCorn" ></span>
                                            </span>
                                        </div>
                                        <input type="text" class="form-control" id="expectNumber" maxlength="20" oninput="value=value.replace(/[^\d]/g,'')">
                                    </div>

                                     <label class="control-label marginSm" >备注项目:</label>
                                     <input type="text" class="form-control" id="modifyRemark" maxlength="100">
                                </div>
                            </form>

                            <div class="form-inline borderTop" id="modifyObjectTime">
                                <label class="control-label">开始时间：</label>
                                <div class="input-group">
                                    <input type="button" class="form-control" id="modifyStartTime" placeholder="开始时间">
                                    <span class="input-group-addon">
                                       <span class="dateCorn" ></span>
                                    </span>
                                </div>
                                <label class="control-label">结束时间：</label>
                                <div class="input-group">
                                    <input type="button" class="form-control" id="modifyEndTime" placeholder="结束时间">
                                     <span class="input-group-addon">
                                       <span class="dateCorn" ></span>
                                    </span>
                                </div>
                                
                            </div>

                            <div class="form-inline borderTop" id="modifyObjectPerson">
                                <label class="control-label">责任人：</label>
                                <div class="input-group">
                                    <input type="button" class="form-control" id="responsePerson">
                                     <button class="input-group-addon" style="height: 33px;">
                                       <span class="responseCorn" ></span>
                                    </button>
                                </div>
                                <label class="control-label">确认人：</label>
                                <div class="input-group">
                                    <input type="button" class="form-control" id="confirmPerson">
                                    <button class="input-group-addon" style="height: 33px;">
                                        <span class="confirmCorn" ></span>
                                    </button>
                                </div>
                            </div>

                           <div class="col-xs-12">
                                <div class="label-head" style="width:12%;float:left;">
                                    标签:
                                </div>
                                <div class="label-main label-list" style="width:85%;float:left;">
                                    <ul class="modfily-ul label-ul">
                                        <li class="label-list-ul label-modify">
                                             <input type="text" id="new-labe"  class="new_label" term="" style="display:none;">
                                             <button class="editLabel" type="button"><i class="fa fa-plus"></i></button>           
                                        </li>
                                    </ul>          
                  
           
                                </div>
                            </div>

                            <div class="row"><h4 class="col-xs-6"> 相关文档 </h4>
                                <span class="modifyDocumentAdd  pull-right"><input type="file"  class="fileUploadBtn" id="fileUpload" name="files[]" multiple ></span>
                                <div class="progress"><div class="bar" style="width:0%;"></div></div>
                            </div>
                            <hr class="noMarginTop"  />
                            <table class="table table-hover" id="modifyObjectDocument">
                            </table>

                            <h4>意见</h4>
                            <hr class="noMarginTop"/>
                            <textarea class="form-control" id="modifyMessage"  rows="2" maxlength="250"></textarea>

                            <h4>操作日志</h4>
                            <hr class="noMarginTop"/>
                            <table class="table table-hover" id="modifyObjectLog">
                            </table>
                        </div>
                        
                     <div class="modal-footer fifty-fifty">
                            <a class="btn btn-transparency " data-dismiss="modal" id="objectModifyCancel">取消</a>
                            <a class="btn btn-transparency " term="1" id="objectModifySave">保存</a>
                            <a class="btn btn-transparency " term="2" id="objectModifyCommit">提交</a>
                     </div>
                </div>
